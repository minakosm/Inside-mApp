{
  "expo": {
    "name": "Inside-mApp",
    "slug": "inside-mApp",
    "description": "This app was developed in the context of my thesis with title 'Implementation of an indoor positioning system for area classification, using mobile phone sensors'",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/insideMappIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/insideMappSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.HIGH_SAMPLING_RATE_SENSORS"
      ],
      "package": "com.minakosm.InsidemApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
